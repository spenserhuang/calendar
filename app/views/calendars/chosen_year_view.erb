<div  class="wrapper-div">
  <ul>
    <% @chosen_year_calendar.each do |calendar| %>
      <li><%= calendar.date_value %></li>
      <% current_user.events.each do |event| %>
        <% if event.event_date_value == calendar.date_value %>
          Event: <a href="/events/<%=event.id%>"><%= event.title %></a>
        <% end %>
      <% end %>
    <% end %>
  </ul>
</div>